                                        <InputText @bind-Value="@trademarkModel.applicantInformation.ApplicantMobNum" placeholder="Mobile Number" class="form-floating form-txt" id="applicantMobNum" required />
                                        <ValidationMessage For="@(() => trademarkModel.applicantInformation.ApplicantMobNum)" />

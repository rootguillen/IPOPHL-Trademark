                                            <UploaderAsyncSettings SaveUrl="api/FileAction/Save" RemoveUrl="api/FileAction/Remove">
                                            </UploaderAsyncSettings>
                                        </SfUploader>
                                    </div>
                                </div>
                            </div> <!--end of Trademark Information-->
                            @if (claimConventionPriority)
                            {
                                <Modal @ref="modal" Title="Claim of Convention Priority" IsVerticallyCentered="true">
                                    <BodyTemplate>
                                        @foreach (var model in TrademarkModels)
                                        {
                                            <div class="row p-3">
                                                <div class="row">
                                                    <div class="form-floating m-2">
                                                        <SfTextBox @bind-Value="@trademarkModel.claimConventionPriority.PriorForeignApplicationNo" Placeholder="Prior Foreign Application No." FloatLabelType="FloatLabelType.Auto"></SfTextBox>
                                                        <ValidationMessage For="@(() => @trademarkModel.claimConventionPriority.PriorForeignApplicationNo)" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-floating m-2">
                                                        <SfDatePicker @bind-Value="@trademarkModel.claimConventionPriority.DateValue" TValue="DateTime?" Placeholder="Foreign Filing Date"></SfDatePicker>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-floating m-2">
                                                        <SfDropDownList TValue="string" TItem="ResidenceDataModel" PopupHeight="auto" PopupWidth="auto" Placeholder="Country" DataSource="@CountryListData">
                                                            <DropDownListFieldSettings Value="CountryId" Text="CountryName"></DropDownListFieldSettings>
                                                        </SfDropDownList>
                                                    </div>
                                                </div>
                                            </div>
                                        }

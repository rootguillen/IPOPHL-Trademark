                                            <label class="ms-2" for="markTitle">Title of the Mark</label>
                                            <ValidationMessage For="@(() => trademarkModel.trademarkInformation.Title)" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col form-floating mb-2">
                                            <InputText @bind-Value="@trademarkModel.trademarkInformation.ClaimColors" placeholder="Claim of Colors (ex. #ffffff, #000000)" class="form-control" id="claimColors" required />
                                            <label class="ms-2" for="claimColors">Claim of Colors (ex. #ffffff, #000000)</label>
                                            <ValidationMessage For="@(() => trademarkModel.trademarkInformation.ClaimColors)" />
                                        </div>
                                    </div>

                                            <ValidationMessage For="@(() => trademarkModel.trademarkInformation.TranslationTransliteration)" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col form-floating mb-2">
                                            <InputText @bind-Value="@trademarkModel.trademarkInformation.Disclaimer" placeholder="Disclaimer" class="form-control" id="claimColors" required />
                                            <label class="ms-2" for="claimColors">Disclaimer</label>
                                            <ValidationMessage For="@(() => trademarkModel.trademarkInformation.Disclaimer)" />
                                        </div>
                                    </div>
